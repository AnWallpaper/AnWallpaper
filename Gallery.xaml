<Page x:Class="AnWallpaper.Gallery"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AnWallpaper"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Gallery">
    <Grid>
        <ScrollViewer x:Name="WindowPanel" VerticalScrollBarVisibility="Hidden" HorizontalScrollBarVisibility="Hidden" Margin="0,34,0,0">
            <UniformGrid x:Name="CardPanel" Rows="0" Columns="3" Margin="10"/>
        </ScrollViewer>
    </Grid>
</Page>
