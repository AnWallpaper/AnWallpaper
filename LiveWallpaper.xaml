<Window x:Class="AnWallpaper.LiveWallpaper"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AnWallpaper"
        mc:Ignorable="d"
        Title="MainWindow" WindowStyle="None" WindowState="Maximized" AllowsTransparency="True" Background="Transparent"
        Loaded="Window_Loaded" ResizeMode="NoResize">
    <Grid>
        <MediaElement Name="BackgroundVideo" LoadedBehavior="Manual" UnloadedBehavior="Manual" Stretch="Fill" MediaEnded="BackgroundVideo_MediaEnded"/>
    </Grid>
</Window>
